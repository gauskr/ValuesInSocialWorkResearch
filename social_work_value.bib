@article{ WOS:000511335700002,
Author = {Gupta, Anoop},
Title = {Some dangers of applying knowledge in social work practice},
Journal = {JOURNAL OF PROGRESSIVE HUMAN SERVICES},
Year = {2019},
Volume = {30},
Number = {3},
Pages = {183-196},
Abstract = {The following question was posed, has social science research helped or
   harmed us? Social work research was used for the purpose of
   illustration. Three hazards of applying social work basic research were
   outlined: (1) It is bias (reflecting the left-leaning values of many
   academics) and leads to the reduction in our freedoms; (2) the social
   work bias is rooted in a supposed moral high ground called social
   justice; and (3), the voice of the non-academics becomes deflated, as
   epistemic elitism is used to foist views upon us by the political class,
   undermining common cultures. On the heels of the three points,
   conclusions were offered to consider if any moderating factors exist to
   address the concern about the loss of freedoms discussed.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Gupta, A (Corresponding Author), Univ Windsor, Fac Educ, Windsor, ON, Canada.
   Gupta, A (Corresponding Author), Univ Windsor, Dept Psychol, Windsor, ON, Canada.
