@article{ WOS:000590839700001,
Author = {Ringland, Valerie and Lewis, Michael A. and Dunleavy, Daniel},
Title = {Beyond the p-value: Bayesian Statistics and Causation},
Journal = {JOURNAL OF EVIDENCE-BASED SOCIAL WORK},
Year = {2021},
Volume = {18},
Number = {3},
Pages = {284-307},
Month = {MAY 4},
Abstract = {Statistical paradigms limit the perspective and tools social work
   researchers use to study the world and answer questions impacting people
   and policy. Currently, quantitative social work researchers
   overwhelmingly rely on the frequentist paradigm of statistics. This
   paper discusses foundational differences between the frequentist and
   Bayesian statistical paradigms, describes basic concepts of Bayesian
   analysis, compares Bayesian and frequentist statistical analysis for a
   sample social work problem, and introduces two types of causal analyses
   built on Bayesian statistical thinking: counterfactual causality, and
   causality based on work by computer scientist Judea Pearl. Implications
   for social work research are discussed.},
