@article{ WOS:000687664000001,
Author = {Dunleavy, Daniel J.},
Title = {The Cultivation of Social Work Knowledge: Toward a More Robust System of
   Peer Review},
Journal = {FAMILIES IN SOCIETY-THE JOURNAL OF CONTEMPORARY SOCIAL SERVICES},
Year = {2021},
Volume = {102},
Number = {4},
Pages = {556-568},
Month = {OCT},
Abstract = {In a recent issue of Families in Society, Caputo argues for the
   ``centrality{''} of peer review in the cultivation of social work
   knowledge. Specifically, he favors the double-blind model of peer
   review. In this paper, I argue that social work should adopt a more
   dynamic set of reviewing practices. First, I define some terminology and
   discuss the limitations of the current double-blind model. Then, I
   describe recent trends in peer review, which I argue foster a more
   robust and open system. I frame this discussion within the context of
   the wider open science movement and urge social workers to engage with
   these scholarly practices. In line with these practices and values, a
   freely accessible preprint is available at:
   https://osf.io/preprints/socarxiv/rvqze/},
