@article{ WOS:A1992JR11400004,
Author = {BRYAN, DL and AJO, AA},
Title = {THE ROLE-PERCEPTION OF AFRICAN-AMERICAN FATHERS},
Journal = {SOCIAL WORK RESEARCH \& ABSTRACTS},
Year = {1992},
Volume = {28},
Number = {3},
Pages = {17-21},
Month = {SEP},
Note = {CONF - FROM THE ATLANTIC TO THE MISSISSIPPI : THE SOCIAL WORK
   CONNECTION, ASHEVILLE, NC, MAY, 1991},
Abstract = {This study examines the social and contextual factors that influence the
   role perceptions of black fathers. Age, education, employment, and
   income were the identified independent variables. A cross-sectional
   survey research design was used. A multistage cluster sampling technique
   was implemented to select 50 black fathers to complete a
   self-administered questionnaire. The respondent fathers were selected
   from two professional organizations and two homeless shelters in
   metropolitan Atlanta. Pearson correlation and chi-square test statistics
   were computed to determine if significant relationships existed between
   each independent variable and the role perceptions of black fathers.
   Results indicated no significant relationship between age or education
   and the role perceptions of black fathers. The number of hours per week
   of work and the income of the black father were found to be
   significantly related at the .05 level to the saliency of role
   perceptions for respondent fathers. Implications for social work
   knowledge, theory, and practice are discussed using humanistic values,
   the Afrocentric perspective, the autonomous social work practice model,
   and planned change.},
