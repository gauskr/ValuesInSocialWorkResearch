@article{ WOS:000321746700008,
Author = {Anderson-Nathe, Ben and Gringeri, Christina and Wahab, Stephanie},
Title = {Nurturing ``Critical Hope{''} in Teaching Feminist Social Work Research},
Journal = {JOURNAL OF SOCIAL WORK EDUCATION},
Year = {2013},
Volume = {49},
Number = {2},
Pages = {277-291},
Abstract = {Despite the congruence between critical feminist values and the cardinal
   values of the social work profession, feminist research in social work
   has lagged behind its feminist cousins in the social sciences,
   particularly in terms of critical uses of theory, reflexivity, and the
   troubling of binaries. This article presents as praxis our reflections
   as researchers, teachers, and feminists inside social work. We draw from
   a review of feminist social work research and offer suggestions for
   teaching critical feminist approaches in social work research.
   Incorporating critical feminist values and research practices into
   social work research courses creates the potential for greater
   integration of research, practice, and the principal values of our
   profession.},
