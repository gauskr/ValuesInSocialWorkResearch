@article{ WOS:000413899700015,
Author = {Evans, Tony and Hardy, Mark},
Title = {The ethics of practical reasoning-exploring the terrain},
Journal = {EUROPEAN JOURNAL OF SOCIAL WORK},
Year = {2017},
Volume = {20},
Number = {6, SI},
Pages = {947-957},
Note = {5th European Conference on Social Work Research, Ljubljana, SLOVENIA,
   APR 22-24, 2015},
Abstract = {Social work has been under sustained scrutiny regarding the quality of
   decision-making. The assumption is that social workers make poor quality
   decisions. And yet our knowledge and understanding of how social workers
   make decisions is, at best, partial. In our view, examination of
   practitioner decision-making will be enhanced by considering the role
   that ethics plays in practical judgement in practice. Although there has
   been significant work regarding the role of values and ethics in
   practice, this work tends to idealize morality, setting up external
   standards by which practice is judged. In this paper, we will argue that
   ethics in practice needs to be understood as more than simply the
   operationalizing of ideal standards. Ethics also entails critical
   engagement with social issues and can challenge idealized statements of
   values. We outline the idea of the ethical dimension of practical
   reasoning, consider its relationship to professional discretion,
   judgements and decision-making and argue that this opens up an area of
   investigation that can illuminate the interaction between practice and
   ethical thinking and reflection in novel and - for social work, at least
   - unconventional ways.},
